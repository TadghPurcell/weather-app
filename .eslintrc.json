{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "allowTernary": true,
    "func-names": 0,
    "no-console": "off",
    "no-unused-vars": "warn"
  }
}
